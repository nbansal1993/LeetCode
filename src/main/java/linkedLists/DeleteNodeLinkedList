/**
 * Definition for singly-linked list.
 * public class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) { val = x; }
 * }
 */
class Solution {
    public void deleteNode(ListNode node) {
        if(node.next!=null){
            ListNode temp = node.next;
            node.val = temp.val;
            node.next = temp.next;
            temp.next=null;
        } 
    }
}
