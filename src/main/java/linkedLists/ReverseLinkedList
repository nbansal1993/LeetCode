/**
 * Definition for singly-linked list.
 * public class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) { val = x; }
 * }
 */
class Solution {
    public ListNode reverseList(ListNode head) {
        if(head==null)
            return null;
        ListNode prev =null;
        ListNode next=null;
        while(head!=null){
            next = head.next;
            head.next =prev;
            prev = head;
            head= next;
        }
        return prev;
    }
}
